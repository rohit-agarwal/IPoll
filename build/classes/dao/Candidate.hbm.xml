<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dao">
  <class name="Candidate">
  	<id name="candidateID" column="candidateId" type="string">
  		<generator class="hilo" />
  	</id>
  	<property name="constituency" column="constituency" type="long" not-null="true" />
  	<property name="symbol" column="symbol" type="string" not-null="true" />
  	<property name="user" column="user" type="string" not-null="true" />
  	<property name="voterid" column="voterid" type="string" not-null="true" />
  	
  </class>
</hibernate-mapping>